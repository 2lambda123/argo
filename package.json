{
    "name": "argo-trading",
    "description": "HTML5 interface for the OANDA platform",
    "version": "4.4.0",
    "main": "./main.js",
    "bin": {
        "argo-trading": "./bin/argo-trading",
        "argo-trading-standalone": "./bin/argo-trading-standalone"
    },
    "preferGlobal": true,
    "repository": "git@github.com:albertosantini/argo.git",
    "keywords": [
        "trading",
        "forex",
        "fxtrade",
        "oanda"
    ],
    "author": "Alberto Santini",
    "license": "MIT",
    "engines": {
        "node": ">=6"
    },
    "dependencies": {
        "async": "~2.1.5",
        "body-parser": "~1.17.1",
        "express": "~4.15.2",
        "faye-websocket": "~0.11.1",
        "flic": "~1.3.7",
        "limiter": "~1.1.0",
        "request": "~2.81.0",

        "angular": "~1.6.3",
        "angular-mocks": "~1.6.3",
        "d3": "~4.7.3",
        "techan": "~0.8.0",
        "tachyons": "~4.6.2"
    },
    "optionalDependencies": {
        "electron": "~1.6.3"
    },
    "scripts": {
        "test": "npm run lint && npm run karma",
        "start": "node src/server/app.js",
        "debug": "node --inspect src/server/app.js",
        "electron": "electron .",
        "lint": "eslint main.js karma.conf.js src/**/*.js",
        "karma": "karma start --single-run",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "rollup": "rollup -c"
    },
    "devDependencies": {
        "conventional-changelog-cli": "^1.3.1",
        "eslint": "^3.17.0",
        "eslint-plugin-angular": "^1.6.2",
        "mocha": "^3.2.0",
        "chai": "^3.5.0",
        "karma": "^1.5.0",
        "karma-mocha": "^1.3.0",
        "karma-chai": "^0.1.0",
        "karma-chrome-launcher": "^2.0.0",
        "rollup": "^0.41.6"
    }
}
