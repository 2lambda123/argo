{
    "name": "argo-trading",
    "description": "HTML5 interface for the OANDA platform",
    "version": "3.1.0",
    "main": "./main.js",
    "bin": {
        "argo-trading": "./bin/argo-trading",
        "argo-trading-standalone": "./bin/argo-trading-standalone"
    },
    "preferGlobal": true,
    "repository": "git@github.com:albertosantini/argo.git",
    "keywords": [
        "trading",
        "forex",
        "fxtrade",
        "oanda"
    ],
    "author": "Alberto Santini",
    "license": "MIT",
    "engines": {
        "node": ">=4"
    },
    "dependencies": {
        "async": "~1.5.2",
        "body-parser": "~1.15.0",
        "electron-prebuilt": "~0.36.7",
        "express": "~4.13.4",
        "faye-websocket": "~0.10.0",
        "flic": "~1.1.2",
        "limiter": "~1.1.0",
        "request": "~2.69.0"
    },
    "scripts": {
        "test": "npm run lint && npm run karma && npm run snyk",
        "start": "node src/server/app.js",
        "electron": "electron .",
        "lint": "eslint main.js karma.conf.js src/**/*.js",
        "karma": "karma start --single-run",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
        "snyk": "snyk --dev test"
    },
    "devDependencies": {
        "conventional-changelog-cli": "~1.0.0",
        "eslint": "~2.0.0-rc.1",
        "eslint-config-eslint": "~3.0.0",
        "eslint-plugin-angular": "~0.15.0",
        "mocha": "~2.4.5",
        "chai": "~3.5.0",
        "karma": "~0.13.19",
        "karma-mocha": "~0.2.1",
        "karma-chai": "~0.1.0",
        "karma-phantomjs-launcher": "~1.0.0",
        "phantomjs-prebuilt": "~2.1.4",
        "snyk": "~1.9.1"
    }
}
